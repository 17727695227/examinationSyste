<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

    <hibernate-mapping>
         
         <class name="com.ds.model.BlankDatabase" table="blank">
              
                <id name="id" column="id" type="string">
                   <generator class="uuid"></generator>
              </id>
              <property name="section" column="section" type="string"></property> 
               
              <property name="question" column="question" type="string"></property> 
              <property name="answer" column="answer" type="string"></property>
             
           
       </class>
            
    </hibernate-mapping>



